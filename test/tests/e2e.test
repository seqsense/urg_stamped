<?xml version="1.0"?>
<launch>
  <arg name="publish_intensity" default="true" />

  <env name="GCOV_PREFIX" value="/tmp/gcov/e2e_intensity_$(arg publish_intensity)" />

  <test test-name="e2e_intensity_$(arg publish_intensity)" pkg="urg_stamped" type="e2e" time-limit="180.0" />

  <node name="urg_stamped" pkg="urg_stamped" type="urg_stamped" respawn="true" output="screen">
    <param name="ip_address" value="127.0.0.1" />
    <param name="publish_intensity" value="$(arg publish_intensity)" />
  </node>
</launch>
